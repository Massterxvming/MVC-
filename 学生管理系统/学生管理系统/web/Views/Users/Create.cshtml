@model DBA.Model.Users
@{
    ViewBag.Title = "Create";
    Layout = null;
}


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
    <script src="../../Scripts/jquery-1.7.1.min.js"></script>
<link href="/Content/css/style.css" rel="stylesheet" type="text/css" />
</head>

<body class="add_test_img"  >

	
    
    <div class="formbody">
    
    
    <form method="post" action="/Users/Edit">
    <ul class="forminfo">
       
          <li><label>学号</label>@Html.TextBoxFor(m => m.LoginName, new { @class = "dfinput" })</li>
      
     
         <li><label>姓名</label>@Html.TextBoxFor(m => m.UserName, new { @class = "dfinput" })</li>
        <li><label>性别</label>@Html.TextBoxFor(m => m.Sex, new { @class = "dfinput" })</li>
        <li><label>年龄</label>@Html.TextBoxFor(m => m.Age, new { @class = "dfinput" })</li>
         <li><label>班级</label>@Html.TextBoxFor(m => m.Str1, new { @class = "dfinput" })</li>
        @Html.HiddenFor(m=>m.ID)
    <li><label>&nbsp;</label>
        <button type="submit"  style="width:137px;height:35px;">保存</button>
       </li>
    </ul>
    </form>
    
    </div>


</body>

</html>


