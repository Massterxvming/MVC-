@model List<DBA.Model.Users>


    <table class="table">
    	<thead>
    	<tr>
        <th>序号<i class="sort"></th>
         
        <th>学号</th>
             <th>手机号</th>
            <th>姓名</th>
            <th>性别</th>
            <th>年龄</th>
             <th>班级</th>
             <th>总分</th>
             <th>总分排行</th>
       
        </tr>
        </thead>
        <tbody>
              @if (Model != null && Model.Count > 0)
{
    int i = 0; 
            foreach (var item in Model)
        {
            i++;    
        <tr>
        <td>@i</td>
             
       
            <td>@item.LoginName</td>
             <td>@item.UserNum</td>
             <td>@item.UserName</td>
             <td>@item.Sex</td>
            <td>@item.Age</td>
             <td>@item.Str1</td>
             <td>@item.LoginPSD</td>
             <td>@i</td>
      
        </tr>    
            }
} 
        </tbody>
    </table>
    


    

